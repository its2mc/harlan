;; Dense matrix-vector product

(module
  (define (main)
    (let ((M (vector
              (vector 1 0 0 0)
              (vector 0 1 0 0)
              (vector 0 0 1 0)
              (vector 0 0 0 1)))
          (X (vector 1 2 3 4)))
      ;; With this print in, sometimes the test doesn't break when it
      ;; should.  Best leave it out.
      ;; (println X)
      (let ((Y (kernel ((row M))
                 (reduce + (kernel ((x X) (y row))
                             (* x y))))))
        (println Y)
        (assert (= X Y))
        0))))
