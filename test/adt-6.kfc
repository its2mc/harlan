;; Linked lists of linked lists

(module

  (define-datatype List
    (null)
    (cons int List))

  (define-datatype List2
    (null2)
    (cons2 List List2))
  
  (define (car ls)
    (match ls
      ((null) (assert #f) -1)
      ((cons n _) n)))

  (define (main)
    (let ((ls (cons 5 (null))))
      (let ((ls2 (cons2 ls (null2))))
        (println "hi")))
    0))
